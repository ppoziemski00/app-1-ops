pipeline {
    agent any // Określa, że pipeline może być wykonany na dowolnym agencie

    stages {
        stage('Build') { // Etap budowania
            steps {
                echo 'Building...'
            }
        }
        stage('Test') { // Etap testowania
            steps {
                echo 'Testing...'
            }
        }
        stage('Deploy') { // Etap wdrażania
            steps {
                echo 'Deploying...'
            }
        }
    }
}
